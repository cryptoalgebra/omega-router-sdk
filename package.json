{
  "name": "@cryptoalgebra/omega-router-sdk",
  "description": "SDK for integrating with the Omega Router contracts",
  "author": "cryptoalgebra",
  "keywords": [
    "omega",
    "integral"
  ],
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "prepare": "yarn build",
    "test": "hardhat test",
    "test:integration": "hardhat test test/integration/*.test.ts",
    "test:fork": "hardhat test test/fork/*.test.ts",
    "lint": "yarn prettier",
    "lint:fix": "yarn prettier:fix && yarn forge:fix",
    "prettier": "prettier --check '**/*.ts' && prettier --check '**/*.json'",
    "prettier:fix": "prettier --write '**/*.ts' && prettier --write '**/*.json'"
  },
  "dependencies": {
    "@cryptoalgebra/integral-sdk": "npm:@cryptoalgebra/clamm-boosted-pools-sdk@^0.2.0",
    "@uniswap/permit2-sdk": "^1.4.0",
    "@uniswap/sdk-core": "^7.10.0",
    "@uniswap/v2-sdk": "^4.16.0",
    "@uniswap/v3-sdk": "^3.26.0",
    "viem": "^2.31.3"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "1.0.4",
    "@nomicfoundation/hardhat-foundry": "1.1.2",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@typechain/ethers-v5": "^4.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.7.0",
    "@uniswap/router-sdk": "^2.2.0",
    "bignumber.js": "^9.0.0",
    "chai": "^4.3.7",
    "dotenv": "^17.2.3",
    "ethers": "^5.7.2",
    "hardhat": "2.22.14",
    "hardhat-typechain": "^0.3.5",
    "jsbi": "^3.2.5",
    "mocha": "^10.2.0",
    "prettier": "^2.4.1",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typechain": "^4.0.1",
    "typescript": "^5.9.3"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "sideEffects": false,
  "version": "0.2.5"
}
